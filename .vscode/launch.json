{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/bin/Debug/net8.0/McpDatalustSeq.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "Now listening on: (https?://\\S+)"
      },
      "env": {},
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/src/Views"
      }
    },
    {
      "name": "Attach to .NET Core",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}